<?xml version="1.0"?>
<cd:ProductClass xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" name="ECMWF" last-modified="2023-11-28">
  <cd:Description>ECMWF GRIB product</cd:Description>
  <cd:ProductType name="GRIB">
    <cd:Description>ECMWF GRIB model outputs</cd:Description>
    <cd:ProductDefinition id="GRIB" format="grib" version="0">
      <cd:Description>The format definition for this product is taken from the GRIB file itself.</cd:Description>
      <cd:DetectionRule>
        <!-- GRIB1/GRIB2: centre 98 -> ECMWF -->
        <!-- GRIB2: current supported master table versions are 5, 27-31 -->
        <cd:MatchExpression><![CDATA[all(/, (exists(./grib1) && int(./grib1/centre) == 98) || (exists(./grib2) && int(./grib2/centre) == 98 && (int(./grib2/masterTablesVersion) == 5 || (int(./grib2/masterTablesVersion) >= 27 && int(./grib2/masterTablesVersion) <= 31))))]]></cd:MatchExpression>
      </cd:DetectionRule>
    </cd:ProductDefinition>
  </cd:ProductType>
</cd:ProductClass>
